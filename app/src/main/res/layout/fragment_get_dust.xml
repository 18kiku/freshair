                                <ImageView
                                    android:id="@+id/pm25gradeimage"
                                    android:layout_gravity="center"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_baseline_cached_24" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/pm25valuetext"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/pm25valueword"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                            </LinearLayout>

                            <!--Child LinearLayout 3-->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:gravity="center"
                                android:orientation="vertical">
                                <TextView
                                    android:textColor="@color/white"
                                    android:text="오존"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>

                                <ImageView
                                    android:id="@+id/o3gradeimage"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:layout_gravity="center"
                                    android:src="@drawable/ic_baseline_cached_24" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/o3valuetext"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/o3valueword"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <!--Child LinearLayout 1-->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:gravity="center"
                                android:orientation="vertical">
                                <TextView
                                    android:textColor="@color/white"
                                    android:text="이산화질소"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:id="@+id/no2gradeimage"
                                    android:layout_gravity="center"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_baseline_cached_24" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/no2valuetext"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/no2valueword"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                            </LinearLayout>

                            <!--Child LinearLayout 2-->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:gravity="center"
                                android:orientation="vertical">
                                <TextView
                                    android:textColor="@color/white"
                                    android:text="일산화탄소"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:id="@+id/cogradeimage"
                                    android:layout_gravity="center"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_baseline_cached_24" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/covaluetext"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/covalueword"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                            </LinearLayout>

                            <!--Child LinearLayout 3-->
                            <LinearLayout
                                android:layout_weight="1"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:gravity="center"
                                android:orientation="vertical">
                                <TextView
                                    android:textColor="@color/white"
                                    android:text="아황산가스"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <ImageView
                                    android:id="@+id/so2gradeimage"
                                    android:layout_gravity="center"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_baseline_cached_24" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/so2valuetext"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                                <TextView
                                    android:textColor="@color/white"
                                    android:id="@+id/so2valueword"
                                    android:layout_gravity="center"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="-"
                                    android:textSize="15dp" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>







                <TextView
                    android:textColor="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="\n날씨"
                    android:textSize="15sp"/>



                <androidx.cardview.widget.CardView
                    app:cardBackgroundColor="@color/gray"
                    android:id="@+id/base_cardview2"
                    style="@style/Base.CardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.473"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.021">

                    <!--This is a ConstraintLayout for the entire CardView
                               including the expandable portion-->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="@+id/base_cardview"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.511"
                        tools:layout_editor_absoluteX="-55dp">

                        <!--This is a ConstraintLayout for the fixed portion
                            of the CardView. The elements
                            that lie within the fixed portion of the CardView
                            can be constrained to this layout.-->

                        <!--The following is the expandable portion whose
                            visibility is initially set to 'gone'.
                            The parent LinearLayout contains 3 child LinearLayouts
                            that hold a subject name and an icon each.-->

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/fixed_layout2"
                            android:layout_width="match_parent"
                            android:layout_height="300dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0">


                            <ImageView
                                android:layout_width="150dp"
                                android:layout_height="150dp"
                                android:id="@+id/sky"
                                android:src="@drawable/ic_baseline_cloud_queue_24"
                                app:layout_constraintBottom_toBottomOf="@+id/fixed_layout2"
                                app:layout_constraintEnd_toEndOf="@+id/fixed_layout2"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toStartOf="@+id/fixed_layout2"
                                app:layout_constraintTop_toTopOf="@+id/fixed_layout2"
                                app:layout_constraintVertical_bias="0.0" />

                            <TextView
                                android:textColor="@color/white"
                                android:id="@+id/tmn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textSize="15dp"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="@+id/fixed_layout2"
                                app:layout_constraintEnd_toEndOf="@+id/fixed_layout2"
                                app:layout_constraintHorizontal_bias="0.75"
                                app:layout_constraintStart_toStartOf="@+id/fixed_layout2"
                                app:layout_constraintTop_toTopOf="@+id/fixed_layout2"
                                app:layout_constraintVertical_bias="0.3" />

                            <TextView
                                android:textColor="@color/white"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textSize="15dp"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="@+id/fixed_layout2"
                                app:layout_constraintEnd_toEndOf="@+id/fixed_layout2"
                                app:layout_constraintHorizontal_bias="0.202"
                                app:layout_constraintStart_toEndOf="@+id/tmn"
                                app:layout_constraintTop_toTopOf="@+id/fixed_layout2"
                                app:layout_constraintVertical_bias="0.3" />

                            <TextView
                                android:textColor="@color/white"
                                android:id="@+id/tmx"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
